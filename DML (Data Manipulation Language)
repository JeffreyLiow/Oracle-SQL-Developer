: Insert, Update, Delete

/* INSTR vs LIKE */
UPDATE TEST
SET TYPE = 
case 
    when INSTR(CRON,'policy',1)>0 then 'POLICY'
    when CRON like '%policy%' then 'POLICY'
end    
where INSERT_DATE =  TRUNC(SYSDATE)
;

/* Delete duplicates */
DELETE FROM STG_CRON_LIST
WHERE rowid not in
(SELECT MIN(rowid)
FROM STG_CRON_LIST
GROUP BY CRON, CRONMI, CRONHH, MYTIME,INSERT_DATE)
;
